              name="Real Estate">
        
        <!-- First Level Advertisements Menu -->
        <menuitem id="estate_advertisements_menu" 
                  name="Advertisements">
            
            <!-- Action Menus (Second Level) -->
            <menuitem id="estate_property_menu_action" 
                      action="estate_property_action"/>
        
        </menuitem>

        <!-- First Level Settings Menu -->
        <menuitem id="estate_settings_menu" 
                  name="Settings">
